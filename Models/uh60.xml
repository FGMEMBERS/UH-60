<?xml version="1.0"?>

<PropertyList>

  <path>uh60.ac</path>
  <dump>0</dump>
  <offsets>
    <x-m>3</x-m>
    <z-m>1.15</z-m>
    <pitch-deg>0</pitch-deg>
  </offsets>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/UH-60/Models/Liveries", 10);
    </load>
    <unload>
      livery_update.stop();
    </unload>
  </nasal>

  <animation>
    <object-name>fuselage</object-name>
    <object-name>elevator</object-name>
    <object-name>caches</object-name>
    <object-name>axeL1</object-name> 
    <object-name>axeL2</object-name> 
    <object-name>axeL3</object-name> 
    <object-name>roueG</object-name>
    <object-name>axeR1</object-name> 
    <object-name>axeR2</object-name> 
    <object-name>axeR3</object-name> 
    <object-name>roueD</object-name>
    <object-name>axeT1</object-name> 
    <object-name>axeT2</object-name> 
    <object-name>axeT3</object-name> 
    <object-name>axeT4</object-name> 
    <object-name>roueA</object-name>
    <object-name>doorL</object-name>
    <object-name>doorR</object-name>
    <object-name>windowL1</object-name>
    <object-name>windowL2</object-name>
    <object-name>windowR1</object-name>
    <object-name>windowR2</object-name>
    <object-name>cachetail</object-name>
    <object-name>rotor</object-name>
    <object-name>intermoteurs</object-name>
    <object-name>porteL</object-name>
    <object-name>porteR</object-name>

    <!-- Transparence -->
    <object-name>vitres</object-name>
    <object-name>glaceL</object-name>
    <object-name>glaceR</object-name>
    <object-name>glaceL1</object-name>
    <object-name>glaceL2</object-name>
    <object-name>glaceR1</object-name>
    <object-name>glaceR2</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitres</object-name>
    <object-name>glaceL</object-name>
    <object-name>glaceR</object-name>
    <object-name>glaceL1</object-name>
    <object-name>glaceL2</object-name>
    <object-name>glaceR1</object-name>
    <object-name>glaceR2</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>fuselage</object-name>
    <object-name>elevator</object-name>
    <object-name>caches</object-name>
    <object-name>axeL1</object-name> 
    <object-name>axeL2</object-name> 
    <object-name>axeL3</object-name> 
    <object-name>roueG</object-name>
    <object-name>axeR1</object-name> 
    <object-name>axeR2</object-name> 
    <object-name>axeR3</object-name> 
    <object-name>roueD</object-name>
    <object-name>axeT1</object-name> 
    <object-name>axeT2</object-name> 
    <object-name>axeT3</object-name> 
    <object-name>axeT4</object-name> 
    <object-name>roueA</object-name>
    <object-name>doorL</object-name>
    <object-name>doorR</object-name>
    <object-name>windowL1</object-name>
    <object-name>windowL2</object-name>
    <object-name>windowR1</object-name>
    <object-name>windowR2</object-name>
    <object-name>cachetail</object-name>
    <object-name>rotor</object-name>
    <object-name>intermoteurs</object-name>
    <object-name>porteL</object-name>
    <object-name>porteR</object-name>

    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
  </animation>

  <!-- Interieur -->
  <model>
    <path>Aircraft/UH-60/Models/Interior/interior.xml</path>
    <offsets>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.000 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

  <!-- Rotor principale -->
  <model>
    <path>Aircraft/UH-60/Models/MainRotor/mainrotor.xml</path>
    <offsets>
      <x-m> -3.047 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  1.287 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>  -5.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

  <!-- Rotor de queue -->
  <model>
    <path>Aircraft/UH-60/Models/TailRotor/tailrotor.xml</path>
    <offsets>
      <x-m> 6.772 </x-m>
      <y-m> 0.379 </y-m>
      <z-m> 1.535 </z-m>
      <roll-deg>  -69.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

  <animation>
    <type>rotate</type>
    <object-name>rotor</object-name>
    <property>rotors/main/blade[0]/position-deg</property>
    <center>
       <x-m> -3.047 </x-m>
       <y-m>  0.000 </y-m>
       <z-m>  0.811 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z>  1.000 </z>
    </axis>
  </animation>

  <!-- Portes coté gauche -->
  <animation>
    <type>translate</type>
    <object-name>doorL</object-name>
    <object-name>doorR</object-name>
    <object-name>glaceL</object-name>
    <object-name>glaceR</object-name>
    <property>/instrumentation/doors/passenger/position-norm</property>
    <factor> 1.70 </factor>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Portes coté gauche -->
  <animation>
    <type>translate</type>
    <object-name>windowL2</object-name>
    <object-name>windowR2</object-name>
    <object-name>glaceL2</object-name>
    <object-name>glaceR2</object-name>
    <property>/instrumentation/doors/crew/position-norm</property>
    <factor> -0.3 </factor>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>windowL1</object-name>
    <object-name>windowL2</object-name>
    <object-name>windowR1</object-name>
    <object-name>windowR2</object-name>
    <object-name>glaceL1</object-name>
    <object-name>glaceL2</object-name>
    <object-name>glaceR1</object-name>
    <object-name>glaceR2</object-name>
    <property>/instrumentation/doors/crew/position-norm</property>
    <factor> -0.3 </factor>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>elevator</object-name>
    <property>/controls/flight/elevator</property>
    <interpolation>
      <entry>
        <ind> -1.00 </ind>
        <dep> -9.66 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> 45.88 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m>  5.929 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.625 </z-m>
    </center>
    <axis>
      <x>  0.00 </x>
      <y>  1.00 </y> 
      <z>  0.00 </z>
    </axis>
  </animation>

  <!-- Compression -->
  <animation>
    <type>translate</type>
    <object-name>roueG</object-name>
    <object-name>axeL1</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind> 1.000 </ind>
        <dep> 0.300 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> 0.000 </x>
      <y> 0.000 </y>
      <z> 1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>axeL2</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> -5.00 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -5.083 </x-m>
      <y-m> -0.178 </y-m>
      <z-m>  0.023 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>roueD</object-name>
    <object-name>axeR1</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind> 1.000 </ind>
        <dep> 0.300 </dep>
      </entry>
    </interpolation>
    <axis>
      <x> 0.000 </x>
      <y> 0.000 </y>
      <z> 1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>axeR2</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry>
        <ind> 0.0 </ind>
        <dep> 0.0 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> -5.00 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -5.083 </x-m>
      <y-m>  0.178 </y-m>
      <z-m>  0.023 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>roueA</object-name>
    <object-name>axeT3</object-name>
    <property>controls/flight/rudder</property>
    <factor>  10.0 </factor>
     <center>
       <x-m>  4.358 </x-m>
       <y-m>  0.000 </y-m>
       <z-m> -1.747 </z-m>
     </center>
     <axis>
       <x>  0.000 </x>
       <y>  0.000 </y>
       <z>  1.000 </z>
     </axis>
  </animation>

  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -4.136 </x-m>
      <y-m> -1.344 </y-m>
      <z-m> -1.995 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -4.136 </x-m>
      <y-m>  1.344 </y-m>
      <z-m> -1.995 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueA</object-name>
    <property>gear/gear[2]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m>  4.665 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.989 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

</PropertyList>
