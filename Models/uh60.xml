  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>elevator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry><ind> -0.04 </ind><dep> -9.66 </dep></entry>
      <entry><ind>  0.04 </ind><dep> 45.88 </dep></entry>
    </interpolation>
    <center>
      <x-m>  5.929 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.625 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y> 
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Compression -->
  <animation>
    <name>TrainGauche</name>
    <object-name>roueG</object-name>
    <object-name>axeL1</object-name>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>TrainGauche</object-name>
    <property>/gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.0 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.3 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>axeL2</object-name>
    <property>/gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> -5 </dep></entry>
    </interpolation>
    <center>
      <x-m> -5.083 </x-m>
      <y-m> -0.178 </y-m>
      <z-m>  0.023 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

