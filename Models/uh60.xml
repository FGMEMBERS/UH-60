    <object-name>axeT3</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TrainArriere</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> -10 </factor>
     <center>
       <x-m>  4.358 </x-m>
       <y-m>  0.000 </y-m>
       <z-m> -1.747 </z-m>
     </center>
